    {
        "id": "22d7d7a7d2cc631c",
        "type": "function",
        "z": "4af8f9a7b3325967",
        "name": "Format Lat & Long ",
        "func": "// Debug de binnenkomende payload\nnode.warn(\"üì° GPS Debug - Ontvangen msg.payload: \" + JSON.stringify(msg.payload)); \n\n// **Stap 1: Status bepalen (Automatisch of Manueel)**\nlet status = \"Automatisch\"; // Standaard is Automatisch\nif (msg.payload.source === \"button1\") {\n    status = \"Manueel\"; // ‚úÖ Als het van button komt, status = Manueel\n}\n\n// **Stap 2: GPS Opslaan bij elke meting**  \nif (msg.payload.latitude !== undefined && msg.payload.longitude !== undefined) {\n    context.set(\"lastGPS\", {\n        latitude: msg.payload.latitude,\n        longitude: msg.payload.longitude\n    });\n}\n\n// **Stap 3: Gebruik de laatst bekende GPS als er geen nieuwe is**\nlet gpsData = context.get(\"lastGPS\") || { latitude: null, longitude: null };\n\n// **Stap 4: Controleer of we GPS-co√∂rdinaten hebben**\nif (gpsData.latitude === null || gpsData.longitude === null) {\n    node.warn(\"‚ö†Ô∏è Geen geldige GPS-data beschikbaar voor deze meting!\");\n    return null; // Stop als er nog nooit GPS-data is ontvangen\n}\n\n// **Stap 5: Bouw het bericht voor de worldmap-node**\nmsg.payload = {\n    name: \"GPS Tracker\",\n    lat: gpsData.latitude, // ‚úÖ Gebruik de laatst bekende GPS\n    lon: gpsData.longitude,\n    layer: \"GPS\",\n    icon: \"fa-map-marker\",\n    iconColor: status === \"Automatisch\" ? \"blue\" : \"red\", // üîπ Rood = Manueel, Blauw = Automatisch\n    iconSize: \"large\",\n    status: status\n};\n\nnode.warn(\"‚úÖ GPS Data geformatteerd voor Worldmap: \" + JSON.stringify(msg.payload));\nreturn msg;\n",
